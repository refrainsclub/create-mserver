{
  "name": "create-mserver",
  "version": "1.0.2",
  "description": "Spin up Paper servers with ease",
  "author": "James Blair",
  "license": "MIT",
  "keywords": [
    "create-mserver",
    "papermc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/refrainsclub/create-mserver.git"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "create-mserver": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json",
    "pnpm-lock.yaml"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts,.mjs",
    "format": "prettier '**/*.{ts,mjs,md,json}' --write"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "gradient-string": "^2.0.2",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.5",
    "@types/gradient-string": "^1.1.5",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-isaacscript": "^3.12.2",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
